{
  "name": "vue-bulma-starter",
  "description": "Vue.js starter with webpack and bulma",
  "version": "1.0.0",
  "author": "Alonso Chavarriaga",
  "private": true,
  "scripts": {
    "dev-build": "concurrently \"npm run style\" \"npm run dev\"",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "style": "node-sass sass/main.sass src/main.css"
  },
  "dependencies": {
    "vue": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "bulma": "^0.2.3",
    "concurrently": "^3.1.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "node-sass": "^4.1.1",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9"
  }
}
